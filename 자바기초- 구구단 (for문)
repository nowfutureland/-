		Scanner scan = new Scanner(System.in);
		
//		'몇단'볼지 입력받아서
		System.out.println("몇단 볼래"); 
		int 몇단 = scan.nextInt(); //18
//		'몇까지 곱할지'입력받아서
		System.out.println("몇단 곱할래");
		int 곱할수 = scan.nextInt(); //3
		

		 
		for(int i=1; i<=곱할수;i++) {
			System.out.println(몇단+"*"+i+"="+(몇단*i));
		}
	
		
		
		
		

	}


}
