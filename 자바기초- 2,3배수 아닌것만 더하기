public class FF {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.print("숫자 입력:");
		int num = scan.nextInt();
		int k = 0;
		
		for(int i=1; i<=num; i++) {
			if(i%2!=0 || i%3!=0) {
				System.out.println("현재 i:"+i);
				//i가 2의 배수, 3의 배수가 아닌지
				k = k +i;
				System.out.println(k);
				
			}
		}
		

	}

}
